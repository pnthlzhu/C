//定义结构体
struct stu{
	char *name;	//姓名
	int num;	//学号
	int age;	//年龄
	char group;	//所在学习小组
	float score;//成绩
};

int main(int argc, char *argv[])
{
	//定义结构体变量
	struct stu stu1;
	stu1.name = "hlzhu";
	stu1.num = 106;
	stu1.age = 28;
	stu1.group = 'A';
	stu1.score = 100.0f;

	return 0;
}
